Imports Microsoft.VisualBasic

Public Class fungsiPerkalian
    Public Function Kali(ByVal x As Integer, ByVal y As Integer) As Integer
        Return x * y
    End Function
    Public Function Bagi(ByVal x As Double, ByVal y As Double) As Double
        Return x / y
    End Function
    Public Function Tambah(ByVal x As Integer, ByVal y As Integer) As Integer
        Return x + y
    End Function
    Public Function Kurang(ByVal x As Integer, ByVal y As Integer) As Integer
        Return x - y
    End Function
End Class
